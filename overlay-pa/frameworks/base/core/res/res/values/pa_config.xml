<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Gestures' key codes. If the gesture is not supported set it to 0. -->
    <integer name="config_doubleTapKeyCode">354</integer>

    <!-- Default settings for gestures
         0 - Disabled
         1 - Wake device
         2 - Toggle flashlight
         3 - Toggle airplane mode
         4 - Play/Pause music
         5 - Next music
         6 - Previous music
         7 - Open camera
         8 - Open dialer
    -->
    <integer name="config_doubleTapDefault">143</integer>

    <!-- Whether to intercept inputs on screen off -->
    <bool name="config_enableKeyHandler">true</bool>

</resources>
